<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.ws_incomeandexpense">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ChildItem" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IncomeExpenseActivity"
            android:label="@string/title_activity_income_expense"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InputDataActivity"
            android:label="@string/title_activity_input_data"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>